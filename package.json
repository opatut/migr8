{
  "name": "migr8",
  "version": "0.1.0",
  "description": "",
  "main": "lib",
  "scripts": {
    "prepublish": "npm run validate && npm run build",
    "build": "babel src/ --out-dir lib/",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "cover": "nyc --reporter=lcov --reporter=text --reporter=html mocha src/**/*.test.js",
    "lint": "eslint src/",
    "report-coverage": "cat ./coverage/lcov.info | node_modules/.bin/codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "mocha lib/**/*.test.js",
    "validate": "npm-run-all --parallel lint",
    "watch": "npm run build -- --watch --source-maps=inline",
    "watch:test": "npm run test -- -w"
  },
  "bin": {
    "migr8": "lib"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.7.5",
    "babel-core": "6.7.4",
    "babel-eslint": "^6.0.4",
    "babel-plugin-espower": "2.1.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.7.2",
    "codecov": "1.0.1",
    "cz-conventional-changelog": "1.1.5",
    "eslint": "^2.7.0",
    "eslint-config-jonathanewerner": "1.0.1",
    "eslint-config-wish-technology": "^2.3.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-flow-vars": "^0.4.0",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-mocha": "^2.2.0",
    "eslint-plugin-react": "^5.0.1",
    "ghooks": "1.2.0",
    "mocha": "2.4.5",
    "npm-run-all": "1.7.0",
    "nyc": "6.2.1",
    "power-assert": "^1.3.1",
    "rimraf": "2.5.2",
    "semantic-release": "^4.3.5",
    "validate-commit-msg": "2.5.0"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "es6-promisify": "^4.0.0",
    "find-parent-dir": "^0.3.0",
    "glob-fs": "^0.1.6",
    "knex": "^0.10.0",
    "lodash": "^4.11.2",
    "node-dir": "^0.1.11",
    "pg": "^4.5.5",
    "source-map-support": "^0.4.0",
    "yaml-parser": "^3.5.3"
  },
  "nyc": {
    "exclude": [
      "**/*.test.js",
      "test/**/*.js"
    ]
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "npm run validate -s"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
